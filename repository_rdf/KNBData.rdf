<rdf:Description rdf:about="http://rio.cs.utep.edu/ciserver/ciprojects/sdata/KNBData.rdf#connolly.116.10">
	<rdf:type rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing"/>
